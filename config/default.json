{
    "dbServer": {
        "database": "drive",
        "host": "mongodb://localhost:27017/oa_drive"
    },
    "webServer": {
        "url": "https://dev.openage.in/drive",
        "port": 3003
    },
    "queueServer": {
        "disabled": true
    },
    "support": {
        "name": "Open Age",
        "email": "support@openage.in"
    },
    "store": {
        "provider": "file-store"
    },
    "auth": {
        "provider": "directory"
    },
    "storage": {
        "provider": "file-store"
    },
    "message": {
        "provider": "send-it"
    },
    "providers": {
        "file-store": {
            "dir": "public/files",
            "root": "https://dev.openage.in/drive/files"
        },
        "aws-s3": {
            "accessKeyId": "placeholder",
            "secretAccessKey": "placeholder",
            "bucketName": "placeholder"
        },
        "cloudnary": {
            "cloud_name": "placeholder",
            "api_key": "placeholder",
            "api_secret": "placeholder"
        },
        "directory": {
            "url": "https://dev.openage.in/directory/api"
        },
        "send-it": {
            "url": "https://dev.openage.in/sendIt/api"
        },
	"discover": {
            "url": "https://dev.openage.in/discover/api"
        }
    },
    "logger": {
        "console": {
            "level": "silly",
            "handleExceptions": true,
            "json": false,
            "colorize": true
        }
    },

    "formats": {
        "date": "DD-MM-YYYY"
    },
    "organization": {
        "timeZone": "IST"
    }
    "api": {
        "errors": {}
    }
}
